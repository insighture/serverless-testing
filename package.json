{
  "name": "serverless-testing",
  "version": "1.0.0",
  "description": "AWS Lambda functions with basic unit tests & integration tests",
  "main": "src/handler.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "lint": "eslint . ",
    "lint:fix": "eslint . --fix ",
    "test": "jest --config jest.config.ts",
    "test:coverage": "jest --config jest.config.ts --coverage",
    "test:unit": "jest --config jest.config.ts --testPathPattern=unit-tests",
    "test:integration": "jest --config jest.config.ts --testPathPattern=integration-tests"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^22.5.5",
    "@types/pino": "^7.0.5",
    "aws-sdk-client-mock": "^4.0.1",
    "eslint": "^9.10.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.6.0"
  },
  "dependencies": {
    "@aws-sdk/client-eventbridge": "^3.654.0",
    "@aws-sdk/client-s3": "^3.654.0",
    "@aws-sdk/credential-providers": "^3.654.0",
    "dotenv": "^16.4.5",
    "pino": "^9.4.0"
  }
}
